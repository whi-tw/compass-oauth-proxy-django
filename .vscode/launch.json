  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "name": "Python: Django",
      "type": "python",
      "request": "launch",
      "cwd": "${workspaceFolder}/compass_oauth_proxy",
      "program": "${workspaceFolder}/compass_oauth_proxy/manage.py",
      "args": ["runserver"],
      "django": true
    }
  ]
}
